$TTL 604800 ; 1 week
@ IN SOA ext.comodoo.dcos. admin.comodoo.dcos. (
    3         ; Serial
    604800    ; Refresh
    86400     ; Retry
    2419200   ; Expire
    604800 )  ; Negative Cache TTL

; name servers
@    IN      NS     ext.comodoo.dcos. 

; PTR Records
100 IN        PTR     dcos-cluster.comodoo.dcos. 
100 IN        PTR     ext.comodoo.dcos. 
101 IN        PTR     boot.comodoo.dcos. 
102 IN        PTR     m1.comodoo.dcos.     
103 IN        PTR     a1.comodoo.dcos.     
104 IN        PTR     a2.comodoo.dcos.     
105 IN        PTR     p1.comodoo.dcos.     
106 IN        PTR     p2.comodoo.dcos.     
